                android:text="收藏夹"
                android:textSize="20sp" 
                android:textColor="#444444"/>

            <View
                android:id="@+id/ImageView04"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="#cccccc" />

            <View
                android:id="@+id/ImageView04"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="#cccccc" 
                android:layout_marginLeft="50dp"/>

        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout05"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/RelativeLayout03"
        android:background="#FFFFFF" 
        android:layout_marginTop="10dp">

        <ImageView
            android:id="@+id/ImageView03"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/u14_line" />

        <RelativeLayout
            android:id="@+id/rl_Setting"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:background="#FFFFFF"
             >
			 
            <ImageView
                android:id="@+id/ImageView05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/t12" />

            <View
                android:id="@+id/ImageView04"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="#cccccc" />

            <ImageView
                android:id="@+id/ImageView06"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/ImageView05"
                android:layout_marginRight="16dp"
                android:src="@drawable/to_right_back_img" />

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="31dp"
                android:layout_toRightOf="@+id/ImageView05"
                android:text="设置"
                android:textSize="20sp" 
                android:textColor="#444444"/>

            <View
                android:id="@+id/ImageView07"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="#cccccc" />

        </RelativeLayout>
    </RelativeLayout>
	
    </RelativeLayout>
    
	</ScrollView>
</RelativeLayout>