<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

  
	<RelativeLayout
		    android:id="@+id/r_23"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_below="@+id/r_22"
		    android:layout_marginTop="10dp"
		    >
		
        <RelativeLayout
            android:id="@+id/rl_tt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true" >
			
            <RelativeLayout
                android:id="@+id/LinearLayout06"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_alignParentLeft="true"
               
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:text="订单编号：" 
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"/>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView1"
                    android:layout_below="@+id/textView1"
                    android:text="下单时间：" 
                    android:layout_marginTop="5dp"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView2"
                    android:layout_below="@+id/textView2"
                    android:text="订单金额：" 
                    android:layout_marginTop="5dp"/>

                <TextView
                    android:id="@+id/tvOrderNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView1"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/textView2"
                    android:text="789456123" />

                <TextView
                    android:id="@+id/tvOrderTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView3"
                    android:layout_alignBottom="@+id/textView3"
                    android:layout_alignLeft="@+id/tvOrderNumber"
                    android:text="2014-06-11  10:11" />

            </RelativeLayout>

            <View
                android:id="@+id/View01"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentLeft="true"
                android:layout_alignTop="@+id/LinearLayout06"
                android:background="#cccccc" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/LinearLayout06"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/rl_tt"
            android:background="#ffffff" >

            <View
                android:id="@+id/View03"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="#cccccc" 
                android:layout_marginLeft="10dp"/>

            <ImageView
                android:id="@+id/ImageView01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/to_right_back_img" />

            <View
                android:id="@+id/View02"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="#cccccc" />

            <Button
                android:id="@+id/button2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignLeft="@+id/ImageView02"
                android:layout_alignTop="@+id/ImageView02"
                android:background="@drawable/t13" />

            <ImageView
                android:id="@+id/ImageView02"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignLeft="@+id/View03"
                android:layout_centerVertical="true"
                android:layout_marginLeft="0dp"
                android:background="@drawable/t14"
                android:layout_below="@+id/View03"
                android:layout_marginTop="9dp"/>

            <Button
                android:id="@+id/Button02"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignLeft="@+id/ImageView03"
                android:layout_alignTop="@+id/ImageView03"
                android:background="@drawable/t13" />

            <ImageView
                android:id="@+id/ImageView03"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_below="@+id/View03"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/button2"
                android:background="@drawable/t14"
                android:layout_marginTop="9dp" />
            
            <Button
                android:id="@+id/Button03"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignLeft="@+id/ImageView04"
                android:layout_alignTop="@+id/ImageView04"
                android:background="@drawable/t13" />

            <ImageView
                android:id="@+id/ImageView04"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_below="@+id/View03"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/ImageView03"
                android:background="@drawable/t14"
                android:layout_marginTop="9dp" />

        </RelativeLayout>
        
          <RelativeLayout android:id="@+id/rl_19"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@+id/LinearLayout06"
            android:background="#ffffff">
            
            <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:text="订单编号：" 
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"/>

            <View
                android:id="@+id/View05"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="#cccccc" />

            <TextView
                android:id="@+id/tvOrderStaut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView2"
                android:layout_toRightOf="@+id/textView2"
                android:text="待付款" 
                android:layout_marginLeft="10dp"
                android:textColor="#333333"/>

            <Button
                android:id="@+id/button3"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:background="@drawable/silver_bg"
                android:text="取消订单" 
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:textSize="10sp"/>

            <Button
                android:id="@+id/Button01"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_alignBaseline="@+id/button3"
                android:layout_alignBottom="@+id/button3"
                android:layout_marginRight="10dp"
                android:layout_toLeftOf="@+id/button3"
                android:background="@drawable/silver_bg"
                android:text="付款"
                android:textSize="10sp" />

          </RelativeLayout>
        </RelativeLayout>
        

</RelativeLayout>