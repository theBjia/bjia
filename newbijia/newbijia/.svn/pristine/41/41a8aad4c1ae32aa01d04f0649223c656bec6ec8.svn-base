<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <include
        android:id="@+id/AdreeId"
        layout="@layout/title_bar" />
    
    
    

    <!--  -->
    
    <RelativeLayout 
    android:id="@+id/addreddTypeID"
    android:layout_width="fill_parent"
    android:layout_height="58dp"
    android:background="#ffffff"
    android:layout_below="@id/AdreeId"
    >
   <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_centerVertical="true"
        android:text="王小小"
        android:textColor="#000000"
        android:textSize="16sp"
        android:background="@null"
        android:singleLine="true"
        />

      <View 
         android:layout_width="fill_parent" 
         android:layout_height="1dp"
         android:background="#dbdbdb"
          android:layout_alignParentBottom="true"
          />
</RelativeLayout>

    
    <!--  -->
    
        <RelativeLayout 
    android:id="@+id/addreddphoneTypeID"
    android:layout_width="fill_parent"
    android:layout_height="58dp"
    android:background="#ffffff"
    android:layout_below="@id/addreddTypeID"
    >
   <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_centerVertical="true"
        android:text="13333333333"
        android:textColor="#000000"
        android:textSize="16sp"
        android:background="@null"
        android:singleLine="true"
        />

      <View 
         android:layout_width="fill_parent" 
         android:layout_height="1dp"
         android:background="#dbdbdb"
          android:layout_alignParentBottom="true"
          />
</RelativeLayout>
    <!--  -->
    
    
           <RelativeLayout 
    android:id="@+id/addred2TypeID"
    android:layout_width="fill_parent"
    android:layout_height="58dp"
    android:background="#ffffff"
    android:layout_below="@id/addreddphoneTypeID"
    >
   <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_centerVertical="true"
        android:text="石家庄小康路自提点"
        android:textColor="#000000"
        android:textSize="16sp"
        android:background="@null"
        android:singleLine="true"
        />

      <View 
         android:layout_width="fill_parent" 
         android:layout_height="1dp"
         android:background="#dbdbdb"
          android:layout_alignParentBottom="true"
          />
</RelativeLayout>

           <!--  -->
           
           
                   <RelativeLayout 
    android:id="@+id/addAAressID"
    android:layout_width="fill_parent"
    android:layout_height="58dp"
    android:background="#ffffff"
    android:layout_below="@id/addred2TypeID"
    >
   <Button
        android:layout_width="400dp"
        android:layout_height="54dp"
        android:layout_marginLeft="16dp"
        android:layout_centerVertical="true"
        android:text="添加地址"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:background="#3ab485"
        android:layout_marginTop="22dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/addAfreBtonnoid"
        />

      
</RelativeLayout>
</RelativeLayout>